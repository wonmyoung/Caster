<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="InsertForm">
        <attr name="title" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="insertTextSize" format="dimension" />
        <attr name="insertTextType" format="enum">
            <enum name="email" value="0"/>
            <enum name="password" value="1"/>
            <enum name="shortMessage" value="2"/>
            <enum name="longMessage" value="3"/>
        </attr>
    </declare-styleable>

</resources>